{
  "name": "go-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format:check": "turbo run format:check",
    "format:fix": "turbo run format:fix",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rimraf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/premchand11/go-boilerplate.git"
  },
  "packageManager": "bun@1.2.13",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/premchand11/go-boilerplate/issues"
  },
  "homepage": "https://github.com/premchand11/go-boilerplate#readme",
  "devDependencies": {
    "turbo": "^2.8.7",
    "typescript": "^5.9.3"
  }
}
